{
  "name": "onsecondary-market-web",
  "description": "NFT Exchange",
  "version": "0.2.0",
  "type": "module",
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "node tasks/buildWebAssets.js && vue-cli-service build",
    "deploy": "node deploy.js",
    "api": "NODE_ENV=production node market-api-server/monoserver.js",
    "api-staging": "NODE_ENV=staging node market-api-server/monoserver.js",
    "api-dev": "NODE_ENV=development node market-api-server/monoserver.js",
    "webserver": "NODE_ENV=production node market-api-server/webserver.js",
    "webserver-staging": "NODE_ENV=staging node market-api-server/webserver.js",
    "webserver-dev": "NODE_ENV=development node market-api-server/webserver.js",
    "imageserver": "node market-api-server/imageserver.js",
    "lint": "vue-cli-service lint",
    "test": "NODE_ENV=test mocha 'tests/**/*.js' --timeout 25000",
    "test:frontend": "vue-cli-service test:unit",
    "populateTraitsDB": "node market-api-server/tasks/populateTraitsDB.js",
    "fetchBAYCData": "node market-api-server/tasks/fetchCollectionDataBAYC.js",
    "fetchMAYCData": "node market-api-server/tasks/fetchCollectionDataMAYC.js",
    "fetchDoodlesData": "node market-api-server/tasks/fetchCollectionDataDoodles.js",
    "fetchCoolcatsData": "node market-api-server/tasks/fetchCollectionDataCoolcats.js",
    "fetchNFTData": "node market-api-server/tasks/fetchAnyCollectionData.js",
    "exportMarketOrders": "node market-api-server/tasks/exportMarketOrders.js",
    "importMarketOrders": "node market-api-server/tasks/importMarketOrders.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "bn.js": "^5.2.0",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^7.0.9",
    "mongodb": "^3.6.5",
    "mongoose": "^6.0.12",
    "node-svg2img": "^1.1.0",
    "socket.io": "^3.1.1",
    "socket.io-client": "^3.1.1",
    "tailwindcss": "^1.0.0-beta.4",
    "vibegraph": "^0.29.1",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vue-tailwind": "^1.2.6",
    "web3": "^1.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.7.4",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-unit-mocha": "^4.5.6",
    "@vue/cli-service": "^3.5.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.0.1",
    "chai": "^4.3.4",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "express": "^4.17.1",
    "highlight.js": "^10.2.0",
    "json-beautify": "^1.1.1",
    "mocha": "^8.2.1",
    "vue-template-compiler": "^2.5.21"
  }
}
